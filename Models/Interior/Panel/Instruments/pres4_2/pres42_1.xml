<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>pres4_2.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>n1</object-name>
    <object-name>n2</object-name>
    
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Antonov-An-12/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>n1</object-name>
    <object-name>n2</object-name> 
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>n2</object-name>
    <property>instrumentation/fuel_press/A0/press1</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry> 
      <entry><ind> 4 </ind><dep> 120 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.019 </y-m>
      <z-m> 0.019 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>n1</object-name>
    <property>instrumentation/fuel_press/A0/press2</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry> 
      <entry><ind> 4 </ind><dep> 120 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.019 </y-m>
      <z-m> -0.019 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
