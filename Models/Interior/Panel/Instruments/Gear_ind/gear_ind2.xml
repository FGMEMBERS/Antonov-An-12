<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>Gear_ind2.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>rect1</object-name>
    <object-name>rect2</object-name>
    
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Antonov-An-12/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>rect1</object-name>
    <object-name>rect2</object-name>
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>rect2</object-name>
    <property>/gear/gear/position-norm</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep> -120 </dep></entry> 
      <entry><ind> 0.07 </ind><dep> -115 </dep></entry>
      <entry><ind> 0.80 </ind><dep>  -80 </dep></entry>
      <entry><ind> 0.90 </ind><dep>  -40 </dep></entry>
      <entry><ind> 1.00 </ind><dep>    0 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.023 </y-m>
      <z-m>  0.000 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>rect1</object-name>
    <property>/gear/gear/position-norm</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep> -120 </dep></entry> 
      <entry><ind> 0.07 </ind><dep> -115 </dep></entry>
      <entry><ind> 0.80 </ind><dep>  -80 </dep></entry>
      <entry><ind> 0.90 </ind><dep>  -40 </dep></entry>
      <entry><ind> 1.00 </ind><dep>    0 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000</x-m>
      <y-m> 0.023</y-m>
      <z-m> 0.000</z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
</PropertyList>
