<PropertyList>
  
  <path>warning16.ac</path>
  
  <animation>
    <object-name>cover</object-name>    
    <object-name>text</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>cover</object-name>    
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <red>   0.35 </red>
      <green> 0.35 </green>
      <blue>  0.35 </blue>
    </emission>
  </animation>
  
  <!--Outer Marker I blue -->
  <animation>
    <type>material</type>
    <object-name>text</object-name>
    <condition>
      <property>instrumentation/marker-beacon/outer</property>
    </condition> 
    <emission>
      <red>   2 </red>
      <green> 2 </green>
      <blue>  2 </blue>    
    </emission>
  </animation>
  
</PropertyList>
